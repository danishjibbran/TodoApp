{
  "name": "mymessaging.app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --public-host=localhost --host=localhost --port 44452 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --public-host=localhost --host=localhost --port 44452 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "copy-ngsw-worker": "shx cp ./ngsw-worker.js ./node_modules/@angular/service-worker",
    "build": "npm run copy-ngsw-worker && ng build",
    "build:ssr": "ng run Todo.App:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "cypress:open": "npx cypress open",
    "cypress:e2e": "cypress run --e2e",
    "cypress:uts": "cypress run --component"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.1",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.1",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/service-worker": "^20.0.0",
    "@fortawesome/fontawesome-free": "^6.7.1",
    "bootstrap": "^5.3.3",
    "hammerjs": "^2.0.8",
    "jquery": "^3.6.0",
    "js-sha256": "^0.11.0",
    "moment": "^2.30.1",
    "popper.js": "^1.16.1",
    "run-script-os": "^1.1.6",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "uuid": "^11.0.3",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/node": "^22.10.1",
    "@types/uuid": "^10.0.0",
    "cypress": "^15.2.0",
    "shx": "^0.3.4",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  }
}
